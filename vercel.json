{
  "version": 2,
  "builds": [
    {
      "src": "*.php",
      "use": "vercel-php"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(?:css|png|jpg|jpeg|gif|svg|ico|webmanifest|txt))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1.php"
    }
  ]
}
